            <specular r="0.1" g="0.1" b="0.0" a="1" />
        </LIGHT>


    </LIGHTS>


<TEXTURES>

	<TEXTURE id="weeds">
        <file path="textures/weeds.jpg" />                             <!-- path to file -->
        <amplif_factor s="2.5" t="2.5" />                <!-- x/s, y/t -->
    </TEXTURE>
    <TEXTURE id="brick">
        <file path="textures/stone_wall.jpg" />                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
	
