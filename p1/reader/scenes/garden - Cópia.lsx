    <ILLUMINATION>
        <ambient r="1" g="1" b="0.8" a="1" />
        <background r="0.298" g="0.718" b="0.8" a="1" />
    </ILLUMINATION>

    <LIGHTS>
        <LIGHT id="light1">
            <enable value="1" />
            <position x="50" y="30" z="0" w="1" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1" />
           <diffuse r="0.5" g="0.5" b="0.5" a="1" />
        </LIGHT>
		
		<LIGHT id="light2">
            <enable value="1" />
            <position x="10" y="50" z="0" w="1" />
			<ambient r="0" g="0" b="0" a="1" />
            <diffuse r="0.7" g="0.623" b="0.44" a="1" />
            <specular r="0.1" g="0.1" b="0.0" a="1" />
        </LIGHT>
		
<LIGHT id="light3">
            <enable value="1" />
            <position x="10" y="0" z="50" w="1" />    
			<ambient r="0" g="0" b="0" a="1" />			
