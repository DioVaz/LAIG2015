/**
 * MyTerrain
 * @constructor
 */
function MyTerrain(scene, rad, slices) {
    CGFobject.call(this, scene);

    this.rad = rad || 1;
    this.slices = slices || 8;

    this.initBuffers();
};

MyTerrain.prototype = Object.create(CGFobject.prototype);
MyTerrain.prototype.constructor = MyTerrain;
